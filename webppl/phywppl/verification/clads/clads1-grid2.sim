{
    "label" : "clads1-grid2",

    "description" : "Grid computation for the full ClaDS1 model. Part of verification. No survivorship bias correction. All parameters except for Î» have been integrated out.",
    
     "hyper_parameters" : {
    	"tree"     : "phyjs.bisse_32" ,
	"particles" : 10000,
	"MAX_LAMBDA" : 5,
	"MAX_DIV" : 5,
	"MIN_LAMBDA" : 1e-5,
	"PANDA" : false
    },

    "parameters" : {
